# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{js,jsx,ts,tsx,css,scss,html,json}]
charset = utf-8

# 2 space indentation
[*.js]
indent_style = space
indent_size = 3

[*.jsx]
indent_style = space
indent_size = 3

[*.ts]
indent_style = space
indent_size = 3

[*.tsx]
indent_style = space
indent_size = 3

[*.css]
indent_style = space
indent_size = 3

[*.scss]
indent_style = space
indent_size = 3

[*.html]
indent_style = space
indent_size = 3

[*.json]
indent_style = space
indent_size = 3

[src/app/assets/icons/icons.tsx]
print_width = 1000
